<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.vanvatcorporation.doubleclips.impl.ViewPagerImpl
        android:id="@+id/mainViewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/belowNavigationBar">


    </com.vanvatcorporation.doubleclips.impl.ViewPagerImpl>


    <LinearLayout
        android:id="@+id/belowNavigationBar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:gravity="center">

        <com.vanvatcorporation.doubleclips.impl.NavigationIconLayout
            android:id="@+id/navigationElement1"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="5dp"
            app:navIcon="@drawable/baseline_home_24"
            app:navText="@string/main_homepage">

        </com.vanvatcorporation.doubleclips.impl.NavigationIconLayout>

        <com.vanvatcorporation.doubleclips.impl.NavigationIconLayout
            android:id="@+id/navigationElement2"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="5dp"
            app:navText="@string/main_template"
            app:navIcon="@drawable/baseline_download_24" >
        </com.vanvatcorporation.doubleclips.impl.NavigationIconLayout>

        <com.vanvatcorporation.doubleclips.impl.NavigationIconLayout
            android:id="@+id/navigationElement3"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="5dp"
            app:navText="@string/main_search"
            app:navIcon="@drawable/baseline_search_24">
        </com.vanvatcorporation.doubleclips.impl.NavigationIconLayout>

        <com.vanvatcorporation.doubleclips.impl.NavigationIconLayout
            android:id="@+id/navigationElement4"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="5dp"
            app:navText="@string/main_storage"
            app:navIcon="@drawable/baseline_history_24">
        </com.vanvatcorporation.doubleclips.impl.NavigationIconLayout>

        <com.vanvatcorporation.doubleclips.impl.NavigationIconLayout
            android:id="@+id/navigationElement5"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="5dp"
            app:navText="@string/main_profile"
            app:navIcon="@drawable/baseline_person_24">
        </com.vanvatcorporation.doubleclips.impl.NavigationIconLayout>

    </LinearLayout>







</RelativeLayout>